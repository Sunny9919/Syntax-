// GET Method
$.ajax({
  url: '/YourController/YourGetMethod',
  type: 'GET',
  success: function(response) {
    // Handle the success response
    console.log(response);
  },
  error: function(xhr, status, error) {
    // Handle the error response
    console.error(error);
  }
});

// POST Method
$.ajax({
  url: '/YourController/YourPostMethod',
  type: 'POST',
  data: {
    key1: value1,
    key2: value2
  },
  success: function(response) {
    // Handle the success response
    console.log(response);
  },
  error: function(xhr, status, error) {
    // Handle the error response
    console.error(error);
  }
});
